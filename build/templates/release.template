//SYSTSPRT DD   SYSOUT=*
//SYSTSIN  DD   *
 DELETE 'BREXX.{version}.LINKLIB(REXX)'
 DELETE 'BREXX.{version}.LINKLIB(RX)'
 RENAME 'BREXX.{version}.LINKLIB(RX1)' 'BREXX.{version}.LINKLIB(REXX)'
 RENAME 'BREXX.{version}.LINKLIB(RX2)' 'BREXX.{version}.LINKLIB(RX)'
